{
  "name": "express-server",
  "version": "1.0.0",
  "description": "Temp hoasting",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "cross-env-shell set DEBUG='dev' NODE_ENV=development && nodemon --exec babel-node server/index.js",
    "debug": "cross-env DEBUG=dev && nodemon --exec babel-node server/index.js --inspect",
    "clean": "rm -rf build && mkdir build && npm run copy-docs",
    "build": "npm run clean && npm run migrate && babel -d ./build ./server",
    "pretest": "cross-env NODE_ENV=test && npm run migrate:undo && npm run migrate && npm run seed",
    "test": "cross-env NODE_ENV=test && nyc mocha --timeout 10000 --require @babel/register server/tests --exit",
    "seed": "npm run seed:all",
    "seed:all": "node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "generate:model": "node_modules/.bin/sequelize model:generate",
    "generate:migration": "node_modules/.bin/sequelize migration:generate",
    "generate:seed": "node_modules/.bin/sequelize seed:generate",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "copy-docs": "cp -r server/docs/ build/docs/",
    "testmail": "cross-env DEBUG=dev && nodemon --exec babel-node server/helpers/mail/mail"
  },
  "author": "Jude Violet",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "cloudinary": "^1.14.0",
    "cors": "^2.8.4",
    "cross-env": "^6.0.3",
    "cryptr": "^4.0.2",
    "debug": "^4.1.1",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-async-errors": "^3.1.1",
    "express-form-data": "^2.0.10",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.21",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "os": "^0.1.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^5.5.0",
    "sequelize-slugify": "^0.7.0",
    "sinon": "^7.3.2",
    "stripe": "^7.8.0",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^3.3.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-integer": "^0.1.0",
    "coveralls": "^3.0.5",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^10.0.6",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  }
}